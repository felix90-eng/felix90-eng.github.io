{% extends 'base.html.twig' %}

{% block title %}Transport cost Report{% endblock %}

{% block body %}
<div class ="col-sm-9">

    <h3>Local transportation Report</h3><br><br>

    <table id ="myTable" class="table">
        <thead>
        {% for t in totals %}
        <tr>
            <th><table width ="900px"> <tr><td><p><strong><i> Overall transportation cost = {{t.total}} </i>rwfs</strong></p></td></tr></table>
            </th>
            </tr>
            {% endfor %}
            <tr>
                <th>#</th>
                 <th>Time</th>
                 <th> Destination</th>
                <th>T.Amount of a trip</th>
            </tr>
        </thead>
        <tbody>
        {% for mission in missions %}
            <tr>
                <td>{{ mission.id }}</td>
                 <td>  {{mission.arrivaltime}} </td>
                <td>{{ mission.trip }}</td>
                <td>{{ mission.totalamount }}</td>
            </tr>
        {% else %}
            <tr>
                <td colspan="20">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    </div>

<script>
$(document).ready(function() {
    $('#myTable').DataTable( {
        dom: 'Bfrtip',
        "paging": true,
		"autoWidth": true,
        buttons: [ 'excel',{
            extend: 'pdfHtml5',
            orientation: 'portrait', //portrait
			pageSize: 'A4', //A3 , A5 , A6 , 
            exportOptions: {
         columns: ':visible',
		 search: 'applied',
		 order: 'applied'
    },
            customize: function ( doc ) {
        doc.defaultStyle.fontSize = 14;
        doc.styles.tableHeader.fontSize = 14;
        doc.styles.title.fontSize = 12;
        // Remove spaces around page title
        doc.content[0].text = doc.content[0].text.trim();
         doc.content[1].table.headerRows = 0;
                // Splice the image in after the header, but before the table
                doc.content.splice( 0, 1);
                doc.pageMargins = [20,200,20,30];

                doc['header']=(function() {
							return {
								columns: [
									{
										
                                       image:'data:image/jpg;base64,'
                                             ,
										width: 80,
                                        height:80,
                                        margin:[202,20]
									},
									{
										italics: true,
										text: 'RWANDA HOUSING AUTHORITY',
										fontSize: 16,
                                        width:300,
										margin: [30,120]
									},
									{
										fontSize: 14,
										text: 'Transportation Cost report',
                                        margin:[5,170]
									}
								],
								//margin: 20
							}
						});
                // Data URL generated by http://dataurl.net/#dataurlmaker
            }
        } ]
    } );
} );
</script>

{% endblock %}

