{{ form_start(form) }}

<div class="form-row">

<div class ="col">
   {{ form_row(form.staff,{attr: {class: 'form-control'} }) }}
</div>
<div class ="col">
    {{ form_row(form.position ,{attr: {class: 'form-control'} }) }}
</div>
<div class ="col">
    {{ form_row(form.telephone ,{attr: {class: 'form-control'} }) }}
</div>
</div>


<div class ="form-row">
   <div class ="col">
    {{ form_row(form.purpose,{attr: {class: 'form-control'} }) }}
    </div>
</div>

<div class ="row">
   <div class ="col">
    {{ form_row(form.destination,{attr: {class: 'form-control'} }) }}
    </div>
</div>


<div class ="row">
   <div class ="col">
    {{ form_row(form.timedisparture) }}
    </div>
    <div class ="col">
    {{ form_row(form.arrivaltime) }}
    </div>
    <div class ="col">
    {{ form_row(form.tripdays,{attr: {class: 'form-control'} }) }}
    </div>
</div>

<div class ="row">
