<form method="post" action="{{ path('g_authentications_delete', {'id': g_authentication.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ g_authentication.id) }}">
    <button class="btn">Delete</button>
</form>
